  "acolyte":{
    "name": "Acolyte",
    "proficiencies": ["Insight", "Religion"],
    "languages": 2,
    "tools": [],
    "equipment": [
      {"name": "A holy symbol", "amount": 1}, 
      {"name": "Stick of Incense", "amount": 5}, 
      {"name": "Vestements", "amount": 1},
